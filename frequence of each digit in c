#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    char str[1001];
    scanf("%s",str);
    for(int i=48;i<58;i++){
        int f=0;
        for(int j=0;j<strlen(str);j++){
            if(str[j]==i){
                f++;
            }
        }
        printf("%d ",f);
    }
    return 0;
}
